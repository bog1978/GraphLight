			<xs:selector xpath="EdgeCategory"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<xs:key name="PK_Vertex">
			<xs:selector xpath="Vertex"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<xs:key name="PK_VertexCategory">
			<xs:selector xpath="VertexCategory"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<xs:keyref name="FK_EdgeCategory" refer="PK_EdgeCategory">
			<xs:selector xpath="Edge"/>
			<xs:field xpath="@Category"/>
