		<xs:keyref name="FK_EdgeDst" refer="PK_Vertex">
			<xs:selector xpath="Edge"/>
			<xs:field xpath="@Dst"/>
		</xs:keyref>
		<xs:keyref name="FK_EdgeSrc" refer="PK_Vertex">
			<xs:selector xpath="Edge"/>
			<xs:field xpath="@Src"/>
		</xs:keyref>
		<xs:keyref name="FK_VertexCategory" refer="PK_VertexCategory">
			<xs:selector xpath="Vertex"/>
			<xs:field xpath="@Category"/>
		</xs:keyref>
	</xs:element>
	<xs:complexType name="LgmlEdge">
		<xs:complexContent>
			<xs:extension base="LgmlElement">
				<xs:attribute name="Src" type="xs:string" use="required"/>
				<xs:attribute name="Dst" type="xs:string" use="required"/>
				<xs:attributeGroup ref="EdgeAttrs"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LgmlEdgeCategory">
		<xs:attribute name="Id" type="xs:string" use="required"/>
		<xs:attributeGroup ref="CommonAttrs"/>
		<xs:attributeGroup ref="EdgeAttrs"/>
	</xs:complexType>
	<xs:complexType name="LgmlElement" abstract="true">
		<xs:attribute name="Category" type="xs:string" use="optional"/>
		<xs:attribute name="Label" type="xs:string" use="optional"/>
		<xs:attributeGroup ref="CommonAttrs"/>
	</xs:complexType>
	<xs:complexType name="LgmlGraph">
		<xs:sequence>
			<xs:element name="VertexCategory" type="LgmlVertexCategory" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="EdgeCategory" type="LgmlEdgeCategory" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Vertex" type="LgmlVertex" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Edge" type="LgmlEdge" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LgmlVertex">
		<xs:complexContent>
			<xs:extension base="LgmlElement">
