<Graph xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../GraphLight.Core/Model/LGML/LGML.xsd">
	<VertexCategory Id="process" Shape="Rectangle" Foreground="Black" Background="LightGray" Margin="10" FontSize="18"/>
	<VertexCategory Id="gateway" Shape="Diamond" Foreground="Green" Background="White" FontSize="25"/>
	<VertexCategory Id="initial" Shape="Ellipse" Foreground="White" Background="Green"/>
	<VertexCategory Id="final" Shape="Ellipse" Foreground="White" Background="Red"/>
	<EdgeCategory Id="normal" Weight="1" Stroke="Green" StrokeThickness="3" StrokeStyle="DashDot"/>
	<EdgeCategory Id="conditional" Weight="1" StrokeStyle="Dash" Stroke="Gray" StrokeThickness="1" Foreground="Blue"/>
	<Vertex Id="start" Category="initial" FontSize="18" StrokeStyle="Solid"/>
	<Vertex Id="end" Category="final" FontSize="18" StrokeStyle="Dash"/>
	<Vertex Id="switch" Category="gateway" StrokeStyle="DashDot"/>
	<Vertex Id="service1" Category="process" StrokeStyle="Dot"/>
	<Vertex Id="service2" Category="process" StrokeStyle="Dot"/>
	<Edge Src="start" Dst="switch" Category="normal"/>
	<Edge Src="switch" Dst="service1" Category="conditional" Label="Yes"/>
	<Edge Src="switch" Dst="service2" Category="conditional" Label="No"/>
	<Edge Src="service1" Dst="end" Category="normal"/>
	<Edge Src="service2" Dst="end" Category="normal"/>
</Graph>
